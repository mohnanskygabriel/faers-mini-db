<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>build</actionName>
            <preAction>build-with-dependencies</preAction>
            <packagings>
                <packaging>*</packaging>
            </packagings>
            <goals>
                <goal>package</goal>
                
            </goals>
        </action>
        <action>
            <actionName>run</actionName>
            <preAction>build-with-dependencies</preAction>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xmx5500M -classpath %classpath com.globallogic.faers.app.Cli -extract D:/ZIP/ -extractDestination D:/ZIP/EXTRACTED</exec.args>
                <exec.executable>java</exec.executable>
                <exec.workingdir></exec.workingdir>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -Xmx5500M -classpath %classpath com.globallogic.faers.app.Cli -extract D:/ZIP/ -extractDestination D:/ZIP/EXTRACTED</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
                <exec.workingdir></exec.workingdir>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xmx5500M -classpath %classpath com.globallogic.faers.app.Cli -extract D:/ZIP/ -extractDestination D:/ZIP/EXTRACTED</exec.args>
                <exec.executable>java</exec.executable>
                <exec.workingdir></exec.workingdir>
            </properties>
        </action>
        <action>
            <actionName>clean</actionName>
            <packagings>
                <packaging>*</packaging>
            </packagings>
            <goals>
                <goal>clean</goal>
            </goals>
        </action>
        <action>
            <actionName>rebuild</actionName>
            <packagings>
                <packaging>*</packaging>
            </packagings>
            <goals>
                <goal>clean</goal>
                <goal>package</goal>
            </goals>
        </action>
    </actions>
